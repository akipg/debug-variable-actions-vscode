{
  "name": "debug-variable-right-click-actions",
  "displayName": "debug-variable-right-click-actions",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onDebug"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "debug-variable-right-click-actions.start",
        "title": "Start"
      },
      {
        "command": "debug-variable-right-click-actions.action",
        "title": "Action"
      },
      {
        "command": "debug-variable-right-click-actions.tocsv",
        "title": "To CSV"
      },
      {
        "command": "debug-variable-right-click-actions.topng",
        "title": "To PNG"
      },
      {
        "command": "debug-variable-right-click-actions.topanel",
        "title": "To Panel"
      }
    ],
    "menus": {
      "debug/variables/context": [
        {
          "command": "debug-variable-right-click-actions.action",
          "when": "true"
        },
        {
          "command": "debug-variable-right-click-actions.tocsv",
          "when": "true"
        },
        {
          "command": "debug-variable-right-click-actions.topng",
          "when": "true"
        },
        {
          "command": "debug-variable-right-click-actions.topanel",
          "when": "true"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "dedent": "^1.5.3",
    "jimp": "^0.22.12"
  }
}
